{
    "name": "anomradar/api",
    "description": "AnomRadar REST API - Security Scan Results Management",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-pdo": "*",
        "ext-json": "*",
        "vlucas/phpdotenv": "5.5.0",
        "tecnickcom/tcpdf": "6.6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "10.4.2"
    },
    "autoload": {
        "psr-4": {
            "AnomRadar\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnomRadar\\Api\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
